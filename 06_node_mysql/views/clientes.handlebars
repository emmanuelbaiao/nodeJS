<form action="/cliente/save" method="post">
    <div class="row mb-3">
    <label for="colFormLabelSm" class="col-sm-1 col-form-label col-form-label-sm">Nome:</label>
    
    <input type="text" class="col-sm-11 form-control-sm" placeholder="Nome" name="nome" id=""/>
    
    <br>
    <label>CPF:</label>
    <input type="text" class="form-control" placeholder="CPF" name="cpf" id=""/>
    <br>
    <label>Endereço:</label>
    <input type="text" class="form-control" placeholder="Endereço" name="endereco" id=""/>
    <br>
    <label>E-mail:</label>
    <input type="email" class="form-control" placeholder="E-mail" name="email" id=""/>
    <br>
    <label>Data Nascimento:</label>
    <input type="date" class="form-control" placeholder="Data Nascimento" name="dataNascimento" id=""/>
    </div>
    <br>
    <input type="submit" class="btn btn-outline-success" value="Cadastrar">
</form>
<hr>
<h1>Lista de Clientes</h1>
<div class="table-responsive">
        <table class="table table-striped table-sm">
          <thead>
            <tr>
              <th scope="col">Id Cliente</th>
              <th scope="col">Nome Cliente</th>
              <th scope="col">CPF</th>
              <th scope="col">Endereço</th>
              <th scope="col">E-mail</th>
              <th scope="col">Data Nascimento</th>
            </tr>
          </thead>
<tbody>
    {{#each clientes}}
            <tr>
        <td>{{this.id_cliente}}</td>
        <td>{{this.nome_cliente}}</td>
        <td>{{this.cpf_cliente}}</td>
        <td>{{this.endereco_cliente}}</td>
        <td>{{this.email_cliente}}</td>
        <td>{{this.data_nascimento_cliente}}</td>              
            </tr>
    {{/each}}
</tbody>
        </table>
</div>